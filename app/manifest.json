{
  "name": "codepen-injector",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Codepen Injector",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "Codepen Injector",
    "default_popup": "popup.html"
  }
}
